@model IEnumerable<hello.Models.Employee>
@{
    ViewData["Title"] = "Index";
}
<h1>Employee Details</h1>
<h2>@ViewBag.Message</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">id</th>
            <th scope="col">name</th>
            <th scope="col">email</th>
            <th scope="col">update</th>
            <th scope="col">delete</th>
        </tr>
    </thead>
    @foreach (var employee in Model)
{
            <tr>
                <td scope="row">@employee.id</td>
                <td scope="row">@employee.name</td>
                <td scope="row">@employee.email</td>
                <td scope="row"><a asp-action="Edit" asp-route-id="@employee.id">Edit</a></td>
                <td scope="row"><a asp-action="Delete" asp-route-id="@employee.id" >Delete</a></td>
            </tr>
}
</table>